<%= form_with model: @user, local: true, class: "form" do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <br>
  <br>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <br>
  <br>

  <%= f.submit "Login" %>

<% end%>

<br>
<br>

<%= link_to "Home", root_path, class: "button" %>